<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.zijin.oep.mapper.DepartmentMapper">
    <resultMap id="DepartmentResultMap" type="com.zijin.oep.model.Department">
        <id column="id"  property="id"/>
        <result column="name" property="name"></result>
        <result column="sid" property="sid"></result>
    </resultMap>


    <select id="selectAll" resultMap="DepartmentResultMap">
        select *from tb_department;
    </select>
</mapper>
